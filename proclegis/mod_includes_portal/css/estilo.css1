}

@media screen and (max-width:950px){
  .noticias_home .mais_noticias {
    width: 100%;
  }
}

.noticias_home .mais_noticias .bloco {
  width: 100%;
  display: table;
  padding: 15px 0;
  border-bottom:1px solid #ccc ;
}


/* #endregion*/

/* #region EVENTOS*/
.home_eventos {
  width: 100%;
  background: #f4f4f4;
}

.home_eventos a {
  color: #585757;
}

.home_eventos .bloco{
  width: 30%;
  float: left;
  margin: 1.5%;
  display: table;
}

@media screen and (max-width:700px){
